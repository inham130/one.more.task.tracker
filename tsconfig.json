{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",
    "lib": ["es2019", "dom"],
    "jsx": "react-jsx",
    "declaration": false,
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}